{
  "expo": {
    "name": "Pet Palate Pro",
    "slug": "pet-palate-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.woonht.petpalatepro"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.woonht.petpalatepro",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#FFF7ED"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "androidClientId": "637482238294-vvo42p39h90ali5n6gvk9hll9rihc276.apps.googleusercontent.com",
          "iosUrlScheme": "com.googleusercontent.apps.637482238294-pfhbel1327logvi3bf1u22i39gs0qje9",
          "iosClientId": "637482238294-pfhbel1327logvi3bf1u22i39gs0qje9.apps.googleusercontent.com"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7d52cc93-b9f4-4562-b8ff-ef0907f5912b"
      }
    }
  }
}
